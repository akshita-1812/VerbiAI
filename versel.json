{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      },
      {
        "src": "images/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ]
  }
  